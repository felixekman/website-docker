<header>
    <div class="container">
        <div class="row">
            <div class="col col-md-6 py-6 text-white">
                <h1 class="mb-4">{{model.title}}</h1>
                <div class="lead">{{markdown-to-html model.lead}}</div>
            </div>
        </div>
    </div>
</header>
<div class="container">
    <div class="row py-5">
        <div class="col-12 col-sm-8 pb-3">
            <div class="contact-icons-sprite phone pull-left mb-5"></div>
            {{markdown-to-html model.contentArea1}}
        </div>
        <div class="col-12 col-sm-4 pb-3">
            <div class="contact-icons-sprite mail pull-left mr-2 mb-5"></div>
            {{markdown-to-html model.contentArea2}}
        </div>
    </div>
</div>
<div class="bg-peach">
    <div class="container">
        <div class="row">
            <div class="col-12 col-sm-8 py-6 text-center">
                {{markdown-to-html model.contentArea3}}
