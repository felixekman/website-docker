            <div class="col-12 col-sm-4">
                {{site-image class="phone-screenshot" asset=(object-at model.images 0)}}
            </div>
        </div>
    </div>
</div>
<div class="container">
    <div class="row py-6">
        <div class="col-12 col-sm-8 pb-3">
            <div class="contact-icons-sprite marker pull-left mr-2 mb-5"></div>
            {{markdown-to-html model.contentArea4}}
            <div class="clearfix pt-3"></div>
            <div class="contact-icons-sprite phone pull-left mr-2"></div>
            {{markdown-to-html model.contentArea5 class="pt-3"}}
            <div class="clearfix pt-5"></div>
            <div class="contact-icons-sprite mail pull-left mr-2"></div>
            {{markdown-to-html model.contentArea6 class="pt-2"}}
        </div>
        <div class="col-12 col-sm-4 py-3">
            <p class="d-inline">
                <strong>Our partners:</strong>
            </p>
            <ul class="nav d-inline">
                <li class="nav-item d-inline"><a class="nav-link d-inline pr-0 active" data-toggle="tab" href="#contactPartner1" role="tab" aria-controls="contactPartner1" aria-expanded="true">DACH</a></li>
                <li class="nav-item d-inline"><a class="nav-link d-inline" data-toggle="tab" href="#contactPartner2" role="tab" aria-controls="contactPartner2">Asia Pacific</a></li>
            </ul>
            <div class="tab-content mt-3">
                <div class="tab-pane fade show active" id="contactPartner1" role="tabpanel">
                    {{markdown-to-html model.contentArea7}}
                </div>
                <div class="tab-pane fade" id="contactPartner2" role="tabpanel">
                    {{markdown-to-html model.contentArea8}}
                </div>
            </div>
        </div>
